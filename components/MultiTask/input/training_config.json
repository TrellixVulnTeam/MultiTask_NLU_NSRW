{
    "HuggingFace_model":"xlm-roberta-base",
    "spaCy_model":"en_core_web_sm",
    "scheme":"IB",
    "max_length":-1,
    "dim":256,
    "dropout":0.2,
    "batch_size":128,
    "epochs":8,
    "num_workers":4,
    "learning_rate":1e-4,
    "learning_rate_alpha":1e-2,
    "weight_decay":1e-3,
    "loss_type":"CELoss",
    "gamma_loss":1.0,
    "temperature_loss":1.0,
    "label_smoothing_loss":0.1,
    "warmup_epochs_factor":0.125,
    "early_stopping":1,
    "scheduler_mode":"min",
    "verbose_steps":200,
    "filepath":"output/",
    "device":"cuda",
    "use_amp":1,
    "seed":123
}