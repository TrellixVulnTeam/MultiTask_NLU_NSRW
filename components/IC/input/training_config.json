{
    "HuggingFace_model":"xlm-roberta-base",
    "dropout":0.25,
    "batch_size":512,
    "epochs":4,
    "dataloader_num_workers":4,
    "gradient_accumulation_steps":1,
    "learning_rate":1e-4,
    "weight_decay":1e-3,
